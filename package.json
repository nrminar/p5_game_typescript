{
  "name": "p5js-with-typescript",
  "version": "1.0.0",
  "description": "A template for a p5.js sketch using Typescript",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "predeploy": "rm -rf dist && parcel build ./*.html --public-url https://github.com/nrminar/p5_game_typescript"
  },
  "dependencies": {
    "@types/p5": "1.7.0",
    "p5": "1.7.0",
    "gh-pages": "^6.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.6.1",
    "sass": "^1.69.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "p5js",
    "typescript"
  ]
}
